{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://cpp.moj.gov.uk/staging/prosecutors/json/schemas/definitions.json",
  "definitions": {
    "name": {
      "type": "string"
    },
    "urn": {
      "type": "string"
    },
    "submissionStatus": {
      "type": "string",
      "enum": [
        "PENDING",
        "FAILED",
        "REJECTED",
        "SUCCESS",
        "SUCCESS_WITH_WARNINGS"
      ]
    },
    "nationalInsuranceNumber": {
      "type": "string",
      "pattern": "^(?!BG)(?!GB)(?!NK)(?!KN)(?!TN)(?!NT)(?!ZZ)[A-CEGHJ-PR-TW-Z][A-CEGHJ-NPR-TW-Z][0-9][0-9][0-9][0-9][0-9][0-9][A-D]$"
    },
    "money": {
      "type": "string",
      "pattern": "^(\\+)?([0-9]{1,12})\\.([0-9]{2})$",
      "description": "An amount expressed in pounds and pence. Must be a positively signed or unsigned number of a maximum of 14 digits plus two decimal places"
    }
  }
}